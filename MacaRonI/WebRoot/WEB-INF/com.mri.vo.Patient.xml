<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.mri.vo.Patient" table="Patient">
	<id name="id" type="java.lang.Integer">
		<column name="ID" />
		<generator class="native" />
	</id>
	<property name="mrn" type="java.lang.String">
	<column name="MRN" length="40" />
	</property>
	<property name="name" type="java.lang.String">
	<column name="Name" length="30" />
	</property>
	<property name="age" type="java.lang.Integer">
	<column name="Age" length="10" />
	</property>
	<property name="gender" type="java.lang.String">
	<column name="Gender" length="15" />
	</property>
	<property name="ftype" type="java.lang.String">
	<column name="Ftype" length="15" />
	</property>
	<property name="file" type="java.lang.String">
	<column name="File" length="50" />
	</property>
	</class>
</hibernate-mapping>